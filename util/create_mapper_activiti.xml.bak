		<table tableName="Act_hi_taskinst" domainObjectName="Act_hi_taskinst" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>

		<table tableName="Act_hi_varinst" domainObjectName="Act_hi_varinst" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>

		<table tableName="Act_id_group" domainObjectName="Act_id_group" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>

		<table tableName="Act_id_info" domainObjectName="Act_id_info" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>

